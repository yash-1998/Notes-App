<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notes"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/create_note"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".NOTES"
            android:label="NOTES" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Noteedit"
            android:label="@string/title_activity_noteedit" >
        </activity>
        <activity
            android:name=".Noteview"
            android:label="@string/title_activity_noteview" >
        </activity>
        <activity
            android:name=".Dview"
            android:label="@string/title_activity_dview" >
        </activity>
        <activity
            android:name=".DATABASE"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
